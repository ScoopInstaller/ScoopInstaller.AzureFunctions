{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "root redirect": {
            "matchCondition": {
                "methods": [
                    "GET"
                ],
                "route": "/"
            },
            "responseOverrides": {
                "response.statusCode": "302",
                "response.headers.Location": "https://scoopsearch.github.io/"
            }
        }
    }
}
